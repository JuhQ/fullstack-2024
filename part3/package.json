{
  "name": "part3",
  "version": "50.0.0",
  "description": "expressjs backend sovellus",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "dev:inspect": "cross-env NODE_ENV=development nodemon --inspect index.js",
    "build:ui": "rm -fr dist && cd ../part2 && npm run build && cp -r dist ../part3",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "cross-env NODE_ENV=test node --test"
  },
  "author": "Juha Tauriainen",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin-js": "^1.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.0.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0"
  }
